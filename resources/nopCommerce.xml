<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="NopCommerce UI Automation" verbose="1">

    <listeners>
        <listener class-name="commons.MethodListener"/>
        <listener class-name="reportConfigs.AllureListener"/>
    </listeners>
    <parameter name="userUrl" value="http://localhost:8082/"/>
    <parameter name="adminUrl" value="https://admin-demo.nopcommerce.com"/>

    <test name="Run on FireFox">
        <parameter name="browser" value="firefox" />
        <classes>
            <class name="com.nopcommerce.users.Level_25_Manage_Data_04_JSON_Excel"/>
        </classes>
    </test>


</suite>
